{
  "name": "jasonrundell2018",
  "version": "1.1.0",
  "main": "index.js",
  "description": "2018 jasonrundell.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonrundell/jasonrundell2018.git"
  },
  "scripts": {
    "test": "jest",
    "start": "npm run clean && npm run copy-static-files && parcel ./src/index.html",
    "clean": "rimraf ./dist && shx mkdir ./dist",
    "copy-static-files": "shx cp ./src/robots.txt ./dist/robots.txt && shx cp -r ./src/password ./dist/password && shx cp -r ./src/time ./dist/time && shx cp -r ./src/files ./dist/files",
    "build": "npm run clean && npm run copy-static-files && parcel build ./src/index.html && npm run purifycss",
    "stylelint": "stylelint 'src/scss/**/*.scss'",
    "purifycss": "purifycss dist/src.*.css dist/src.*.js --min --info --out dist/src.final.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aos": "^2.3.4",
    "babel-polyfill": "^6.26.0",
    "history": "^4.9.0",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "parcel-plugin-sw-precache": "^1.1.1",
    "purifycss": "^1.2.6",
    "rimraf": "^2.6.3",
    "sass": "^1.20.1",
    "shx": "^0.3.2"
  }
}
